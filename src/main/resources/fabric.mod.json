	"name": "DuckMod",
	"description": "Duckmod adds ducks to Minecraft!",
	"authors": [
		"bharath2438"
	],
	"contact": {
		"sources": "https://github.com/bharath2438/duckmod"
	},
	"license": "CC0-1.0",
	"icon": "assets/duckmod/icon.png",
